<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CustomerController">
	<children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="1.0">
         <children>
      		<TableView fx:id="tableMinions" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
      			<columns>
      				<TableColumn fx:id="minionId" prefWidth="50.0" text="Minion ID" />
      				<TableColumn fx:id="hostName" prefWidth="50.0" text="Hostname" />
      				<TableColumn fx:id="tag" prefWidth="50.0" text="Tag" />
      				<TableColumn fx:id="publicIP" prefWidth="50.0" text="Public IP" />
      				<TableColumn fx:id="IP" prefWidth="50.0" text="Private IP" />
      				<TableColumn fx:id="CPU" prefWidth="50.0" text="CPU" />
      				<TableColumn fx:id="RAM" prefWidth="50.0" text="RAM" />
      				<TableColumn fx:id="online" prefWidth="50.0" text="Online" />
      				<TableColumn fx:id="select" prefWidth="50.0" text="Select" />
      			</columns>
      		</TableView>
            <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="200.0" prefWidth="200.0" spacing="3.0">
               <children>
                  <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Minion List Info">
                    <content>
                      <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                           <children>
                              <VBox maxWidth="1.7976931348623157E308" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <children>
                              		<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#queryMinions" text="List Minions" VBox.vgrow="ALWAYS" />
                              		<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="List Minions &amp; Basic Info" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                    </content>
                     <font>
                        <Font name="Arial Bold" size="16.0" />
                     </font>
                  </TitledPane>
                  <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Minion Queries">
                     <content>
                        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                           <children>
                              <VBox maxWidth="1.7976931348623157E308" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <children>
                                    <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#queryMinionBasicInfo" text="Minion Basic Info" VBox.vgrow="ALWAYS" />
                                    <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Minion Processes" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                     </content>
                     <font>
                        <Font name="Arial Bold" size="16.0" />
                     </font>
                  </TitledPane>
                  <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Admin Tasks">
                     <content>
                        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                           <children>
                              <VBox maxWidth="1.7976931348623157E308" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <children>
                                    <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#viewAccounts" text="View Minion Logs" VBox.vgrow="ALWAYS" />
                                    <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="View User Logs" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                     </content>
                     <font>
                        <Font name="Arial Bold" size="16.0" />
                     </font>
                  </TitledPane>
               </children>
               <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
